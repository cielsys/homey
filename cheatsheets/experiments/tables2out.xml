          <field type="key-sequence">C-b</field>
          <field type="key-sequence">C-f</field>
        </record-def>
      </item>
      <item>
        <record-def>
          <name>word</name>
          <field type="key-sequence">M-b</field>
          <field type="key-sequence">M-f</field>
        </record-def>
      </item>
      <item>
        <record-def>
          <name>line</name>
